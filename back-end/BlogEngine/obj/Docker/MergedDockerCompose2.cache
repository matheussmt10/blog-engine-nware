name: dockercompose14492515222833773897
services:
  blogengine.api:
    build:
      context: C:\dev\blog-engine-nware\back-end\BlogEngine
      dockerfile: src/BlogEngine.API/Dockerfile
    container_name: blog-engine-api
    depends_on:
      blogenginedb:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      DB_HOST: blogenginedb
      DB_NAME: blog_engine
      DB_PASSWORD: admin
      DB_USER: root
    image: blogengineapi
    networks:
      backend: null
    ports:
      - mode: ingress
        target: 8080
        published: "8002"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\Users\mathe\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\mathe\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  blogenginedb:
    container_name: blog-engine-db
    environment:
      MYSQL_DATABASE: blog_engine
      MYSQL_ROOT_PASSWORD: admin
    image: mysql:latest
    networks:
      backend: null
    ports:
      - mode: ingress
        target: 3306
        published: "8001"
        protocol: tcp
networks:
  backend:
    name: dockercompose14492515222833773897_backend